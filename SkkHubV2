local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "skk hub", HidePremium = false, IntroText = "Skk Hub", SaveConfig = true, ConfigFolder = "OrionTest"})

OrionLib:MakeNotification({
	Name = "Skk",
	Content = "Made by Skk",
	Image = "rbxassetid://4483345998",
	Time = 5
})

local MainTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
MainTab:AddButton({
	Name = "click delete",
	Callback = function()
      		print("button pressed")
loadstring(game:HttpGet('https://raw.githubusercontent.com/Des5dw/A/main/A'))()
  	end    
})
